   rect(440, 515, 20, 20);
   rect(530, 180, 25, 25);
   rect(485, 115, 30, 30);
   rect(465, 475, 30, 30);
   rect(530, 210, 40, 40);
   rect(500, 420, 40, 40);   
   rect(385, 545, 20, 20);
   rect(340, 545, 30, 30);
   rect(285, 370, 25, 25);
   rect(315, 370, 30, 30);
   rect(285, 410, 25, 25);
   rect(315, 410, 35, 35);
   rect(230, 520, 30, 30);
