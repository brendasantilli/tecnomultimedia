   rect(170, 305, 35, 35);
   rect(220, 295, 45, 45);
   rect(30, 200, 40, 40);
   rect(55, 160, 30, 30);
   rect(120, 80, 30, 30);
   rect(155, 55, 40, 40);
   
   //CIRCULOS
   fill(#B31C00);
   circle(400, 280, 60);
   circle(470, 170, 20);
   circle(380, 180, 40);
   
   //TRIANGULO
  // triangle(x1, y1, x2, y2, x3, y3)
